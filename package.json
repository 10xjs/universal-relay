    "build": "npm run update-schema && npm run build:client && npm run build:server",
    "dev": "npm run update-schema && NODE_ENV=development ./node_modules/.bin/webpack-udev-server ./config/webpack/*.webpack.config.babel.js",
    "postinstall": "npm run build"
  },
  "author": "Neal Granger <neal@metalab.co>",
