{
  "name": "universal-relay",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "build:client": "./node_modules/.bin/webpack --bail --config config/webpack/client.webpack.config.babel.js",
