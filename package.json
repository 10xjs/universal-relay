    "babel-preset-metalab": "^0.2.1",
    "babel-relay-plugin-loader": "^0.7.3",
    "bluebird": "^3.3.5",
    "exports": "0.0.1",
    "exports-loader": "^0.6.3",
    "htmlescape": "^1.1.1",
    "imports-loader": "^0.6.5",
    "loader": "^2.1.0",
    "lodash": "^4.11.1",
    "node-fetch": "^1.5.1",
    "node-libs-browser": "^1.0.0",
    "react": "^15.0.1",
